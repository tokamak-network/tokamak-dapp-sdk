{
  "name": "tokamak-dapp-sdk",
  "version": "1.0.5",
  "description": "A library to hold the main logic for a dapp on Titan network",
  "author": "Tokamak Network",
  "license": "GPL-3.0-or-later",
  "repository": "tokamak-network/titan-dapp-sdk",
  "main": "./__commonjs",
